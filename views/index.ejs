<!-- FAQs Section Start -->
<section class="faq-sec mb-120 wow fadeInUp" data-wow-delay="590ms">
  <div class="container-fluid">
    <div class="mb-36">
      <p class="color-sec mb-4p">What’s on your mind!</p>
      <h3 class="fw-500">Frequently Asked Questions</h3>
    </div>
    <div class="faq-sec">
      <div id="accordionBuildingConstructionLeft">
        <div class="faq">
          <% faqs.forEach((faq, index) => { %>
            <div class="faq-block mb-12">
              <a href="#" class="accordion-button <%= index !== 0 ? 'collapsed' : '' %>" data-bs-toggle="collapse"
                 data-bs-target="#faq<%= index %>" aria-expanded="<%= index === 0 %>" aria-controls="faq<%= index %>">
                <%= faq.title %>
              </a>
              <div id="faq<%= index %>" class="accordion-collapse collapse <%= index === 0 ? 'show' : '' %>"
                   aria-labelledby="faq<%= index %>" data-bs-parent="#accordionBuildingConstructionLeft">
                <p><%= faq.description %></p>
              </div>
            </div>
          <% }) %>
        </div>
      </div>
    </div>
  </div>
</section>
<!-- FAQs Section End -->